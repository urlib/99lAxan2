*Feature: Resolution
{
	*rcNameID: =RESOLUTION_DISPLAY
	*UpdateQualityMacro?: TRUE
	*DefaultOption: EPOPT_INPUTRES_360DPI
	*Option: EPOPT_INPUTRES_180DPI
	{
		*rcNameID: =EPN_RCID_INRES_180
		*DPI: PAIR(180, 180)
		*TextDPI: PAIR(180, 180)
		*MinStripBlankPixels: 1
		*SpotDiameter: 120
	}
	*Option: EPOPT_INPUTRES_360DPI
	{
		*rcNameID: =EPN_RCID_INRES_360
		*DPI: PAIR(360, 360)
		*TextDPI: PAIR(360, 360)
		*MinStripBlankPixels: 1
		*SpotDiameter: 160
	}
	*Option: EPOPT_INPUTRES_720DPI
	{
		*rcNameID: =EPN_RCID_INRES_720
		*DPI: PAIR(720, 720)
		*TextDPI: PAIR(720, 720)
		*MinStripBlankPixels: 1
		*SpotDiameter: 180	
	}
}

