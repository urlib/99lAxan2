*Feature: PageColorAdjustmentMode
{
	*FeatureType: DOC_PROPERTY
	*rcNameID: =EPN_RCID_COLOR_ADJUST
	*DefaultOption: ByDriver
	*Option: ByDriver
	{
		*rcNameID: =EPN_RCID_COLOR_CONTROLS
		EXTERN_GLOBAL: *PreAnalysisOptions: 0
	}
	*Option: AutoPhotoFine
	{
		*rcNameID: =EPN_RCID_PHOTO_ENHANCE
		*switch: PageScaling
		{
			*case: None
			{
				EXTERN_GLOBAL: *PreAnalysisOptions: 8
			}
			*case: CustomSquare
			{
				EXTERN_GLOBAL: *PreAnalysisOptions: 0
			}
			*case: FitPage
			{
				EXTERN_GLOBAL: *PreAnalysisOptions: 0
			}
		}
	}
	*Option: None
	{
		*rcNameID: =EPN_RCID_NONE_ADJUST
		EXTERN_GLOBAL: *PreAnalysisOptions: 0
	}
}

